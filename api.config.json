{
  "name": "mle-api",
  "script": "npm",
  "args": "startdev",
  "watch": ".",
  "ignore_watch": ["node_modules"],
  "instances"  : 4,
  "exec_mode"  : "cluster",
  "env": {
    "NODE_ENV": "development"
  },
  "env_production" : {
    "NODE_ENV": "production"
  },
  "max_memory_restart": "150M",
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "error_file": "./logs/mle_api_error.log",
  "out_file": "./logs/mle_api_output.log"
}
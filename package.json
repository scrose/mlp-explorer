{
  "name": "mlp-api",
  "version": "1.0.0",
  "proxy": "http://localhost:3001",
  "main": "./src/app.js",
  "type": "module",
  "private": true,
  "author": {
    "name": "Spencer Rose",
    "email": "support@goruntime.ca",
    "url": "https://goruntime.ca"
  },
  "license": "MIT",
  "repository": "github:scrose/mlp-explorer/",
  "bugs": "https://github.com/scrose/mlp-explorer/issues",
  "scripts": {
    "startpm2": "pm2 start -f --watch --ignore-watch=\\\"node_modules\\\" --name mle ./src/bin/www.js --log ./logs/pm2.log -- -r dotenv/config app.config.js",
    "startdebug": "nodemon --trace-warnings --inspect --trace_gc ./src/bin/www.js -r dotenv/config app.config.js",
    "startdev": "nodemon --trace-warnings ./src/bin/www.js -r dotenv/config app.config.js",
    "start": "node ./src/bin/www.js -r dotenv/config app.config.js",
    "serverstart": "DEBUG=mlp-explorer:* npm run devstart",
    "test": "nyc mocha --exit",
    "coverage": "nyc npm run test",
    "client": "cd client && npm start",
    "build": "cd client && npm run build && serve -s build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "dotenv": "^8.2.0",
    "exif": "~0.6.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.7.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.16",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "node-forge": ">=0.10.0",
    "nodemailer": "^6.4.14",
    "openid-client": "^4.4.0",
    "pg": "^8.5.1",
    "sharp": "^0.27.0",
    "uid-safe": "^2.1.5",
    "uuid": "^8.3.1",
    "xmp-reader": "0.0.3"
  },
  "devDependencies": {
    "busboy": "^0.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "coveralls": "^3.1.0",
    "debug": "~2.6.9",
    "esm": "^3.2.25",
    "formidable": "^1.2.2",
    "helmet": "^4.2.0",
    "keycloak-admin": "^1.14.7",
    "method-override": "^3.0.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "2.2.0",
    "supertest": "^6.0.1"
  }
}

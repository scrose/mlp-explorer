{
  "name": "mle-client",
  "script": "npm",
  "args": "client",
  "cwd": "./client",
  "env": {
    "NODE_ENV": "development"
  },
  "env_production" : {
    "NODE_ENV": "production"
  },
  "max_memory_restart": "150M",

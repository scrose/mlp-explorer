<!DOCTYPE html>
<html>
<head>
    <%- include('../partials/head', {title: content.title}); %>
</head>
<body>
<%- include('../partials/header', {content: content}); %>
<main>
    <table class="records">
        <tbody>
        <tr>
            <th>Logged-in</th>
            <th>Email</th>
            <th>Role</th>
            <th>Current Sign-in</th>
            <th>Last Sign-in</th>
            <th>Created</th>
            <th>Updated</th>
        </tr>
        <% if (users.length === 0) { %>
            <td colspan="8">No users.</td>
        <% } %>
        <% for (const [key, user] of Object.entries(users)) { %>
            <tr>
                <td><%= !!user.session_token %></td>
                <td><%= user.email %></td>
                <td><%= user.role %></td>
                <td><time><%= user.current_sign_in_at %></time></td>
                <td><time><%= user.last_sign_in_at %></time></td>
                <td><time><%= user.created_at %></time></td>
                <td><time><%= user.updated_at %></time></td>
                <td><a href="/users/<%= user.user_id %>">view</a></td>
                <td><a href="/users/<%= user.user_id %>/edit">edit</a></td>
                <td><a href="/users/<%= user.user_id %>/delete">delete</a></td>
            </tr>
        <% } %>
        </tbody>
    </table>
</main>
<%- include('../partials/footer', {content: content}); %>
</body>
</html>

